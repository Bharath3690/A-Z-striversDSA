class Solution {
    public int missingNumber(int[] nums) {
        int sum=0;
        int s1 =  (nums.length*(nums.length+1))/2;
        for(int i=0;i<nums.length;i++){
            if(nums[i]<=nums.length){
                sum+=nums[i];         
            }
        }
        return s1-sum;
        
    }
}
