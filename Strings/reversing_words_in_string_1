class Main {
    public static void main(String[] args) {
       String str= "Hello world welcome to college";
       String[] words = str.split(" ");
       String res = "";
       for(String word:words){
           char[] ch = word.toCharArray();
           String reverse = "";
           for(int i=ch.length-1;i>=0;i--){
               reverse+=ch[i];
           }
           res+=reverse+" ";
       }
       System.out.print(res.trim());
    }
}
